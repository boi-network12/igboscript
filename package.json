{
  "name": "igboscript",
  "version": "1.0.4",
  "displayName": "Igboscript Language Support",
  "description": "A lightweight programming language for writing JavaScript in Igbo",
  "publisher": "kamdi",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "index.js",
  "icon": "icon.png",
  "scripts": {
    "dev": "node bin/igboscript.js dev",
    "build": "next build",
    "start": "next start",
    "igboscript": "node bin/igboscript.js"
  },
  "bin": {
    "igboscript": "bin/igboscript.js"
  },
  "keywords": [
    "igbo",
    "language",
    "javascript",
    "dsl"
  ],
  "author": "kamdi",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "acorn": "^8.15.0",
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "chokidar": "^4.0.3",
    "dotenv": "^17.2.2",
    "fa": "^1.0.1",
    "fs": "^0.0.1-security",
    "glob": "^11.0.3",
    "open": "^10.2.0",
    "path": "^0.12.7",
    "readline": "^1.3.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "igboscript",
        "aliases": [
          "Igboscript",
          "igboscript"
        ],
        "extensions": [
          ".is",
          ".js"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "igboscript",
        "scopeName": "source.igboscript",
        "path": "./syntaxes/igboscript.tmLanguage.json"
      }
    ]
  }
}